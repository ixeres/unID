Find a user!
<%= form_tag find_path, method: 'get' do %>
  <div class="field">
    <%= label_tag :profile_network_id %>
    <%= select_tag :profile_network_id, options_for_select(provider_list) %>
  </div>

  <div class="field">
    <%= label_tag :profile_name %>
    <%= text_field_tag :profile_name %>
  </div>
  <br>

  <div class="actions"><%= submit_tag "Search" %></div>
<% end  %>
