<h1>Welcome To UnID!</h1>
<h2 id="log">Log In</h2>
<div id="logInDiv">
<%= render partial: "sessions/login_form" %>
</div>
<h2 id="sign">Sign Up</h2>
<div id="signUpDiv">
<%= render partial: "users/signup", locals: { user: @user } %>
</div>
<h2 id="forgot">Forgot Your Password</h2>
<div id="forgotPassword">
<%= render partial: "sessions/resetpassword" %>
</div>
